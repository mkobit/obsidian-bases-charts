properties:
  note.Date:
    displayName: Date
  note.Mentions:
    displayName: Mentions
  note.Topic:
    displayName: Topic
views:
  - type: theme-river-chart
    name: News Topics River
    xAxisProp: note.Date
    valueProp: note.Mentions
    seriesProp: note.Topic
    showLegend: true
    filters:
      and:
        - note.Topic != null
