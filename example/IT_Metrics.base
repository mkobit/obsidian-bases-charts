properties:
  note.Time:
    displayName: Time
  note.Server:
    displayName: Server
  note.Load:
    displayName: Load
  note.Status:
    displayName: Status
views:
  - type: heatmap-chart
    name: Server Load Heatmap
    xAxisProp: note.Time
    yAxisProp: note.Server
    valueProp: note.Load
    showLegend: true
    filters:
      and:
        - note.Load != null
  - type: gauge-chart
    name: Server Load Gauge (Aggregated)
    yAxisProp: note.Load
    minVal: 0
    maxVal: 100
    filters:
      and:
        - note.Load != null
